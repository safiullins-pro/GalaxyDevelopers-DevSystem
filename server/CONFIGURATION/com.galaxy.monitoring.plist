<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.galaxy.monitoring</string>
    
    <key>ProgramArguments</key>
    <array>
        <string>/bin/bash</string>
        <string>/Volumes/Z7S/development/GalaxyDevelopers/DevSystem/start_monitoring.sh</string>
    </array>
    
    <key>WorkingDirectory</key>
    <string>/Volumes/Z7S/development/GalaxyDevelopers/DevSystem</string>
    
    <key>RunAtLoad</key>
    <true/>
    
    <key>KeepAlive</key>
    <dict>
        <key>SuccessfulExit</key>
        <false/>
        <key>Crashed</key>
        <true/>
    </dict>
    
    <key>StandardOutPath</key>
    <string>/Volumes/Z7S/development/GalaxyDevelopers/DevSystem/logs/launchd.out.log</string>
    
    <key>StandardErrorPath</key>
    <string>/Volumes/Z7S/development/GalaxyDevelopers/DevSystem/logs/launchd.err.log</string>
    
    <key>EnvironmentVariables</key>
    <dict>
        <key>PATH</key>
        <string>/usr/local/bin:/usr/bin:/bin:/usr/sbin:/sbin:/opt/homebrew/bin</string>
        <key>PYTHONPATH</key>
        <string>/Volumes/Z7S/development/GalaxyDevelopers/DevSystem</string>
    </dict>
    
    <key>StartInterval</key>
    <integer>300</integer>
    
    <key>ThrottleInterval</key>
    <integer>10</integer>
</dict>
</plist>